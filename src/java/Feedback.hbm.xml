<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="bean.Feedback" table="FeedbackTable">
      <id name="fid" column="feedbackID">
          <generator class="increment"/>
      </id>
      
      <property name="fusr" column="feedbackUsername"/>
      <property name="fsub" column="feedbackSubject"/>
      <property name="ftext" column="feedbackText"/>
  </class>
</hibernate-mapping>
