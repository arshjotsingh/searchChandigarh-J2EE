<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="bean.Category" table="CategoryTable">
      <id name="catid" column="categoryID">
          <generator class="increment"></generator>
      </id>
      
      <property name="catname" column="categoryname"></property>
      <property name="desc" column="categorydescription"></property>
      <property name="tags" column="tags"></property>
      
  </class>
</hibernate-mapping>
